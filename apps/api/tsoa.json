{
  "entryFile": "apps/api/src/main.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["apps/api/src/**/*Controller.ts"],
  "spec": {
    "outputDirectory": "swagger",
    "specVersion": 3
  },
  "routes": {
    "routesDir": "apps/api/build"
  },
  "ignore": [
    "**/node_modules/borsh/node_modules/@types/bn.js/**",
    "**/node_modules/@types/node/crypto.d.ts"
  ]
}
