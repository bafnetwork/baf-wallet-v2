{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "api",
  "projects": {
    "frontend-e2e": {
      "root": "apps/frontend-e2e",
      "sourceRoot": "apps/frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontend-e2e/cypress.json",
            "tsConfig": "apps/frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/frontend-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "tsoa": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "rm -r apps/api/build; mkdir apps/api/build"
              },
              {
                "command": "yarn tsoa --configuration apps/api/tsoa.json spec-and-routes && tsc"
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-client": {
      "root": "libs/api-client",
      "sourceRoot": "libs/api-client/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-client/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-client"],
          "options": {
            "jestConfig": "libs/api-client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "autogen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "rm -r libs/api-client/src/build; mkdir libs/api-client/src/build"
              },
              {
                "command": "yarn openapi-generator-cli generate -i ./swagger/swagger.json --generator-name typescript-fetch -o libs/api-client/src/build"
              }
            ]
          }
        }
      }
    },
    "bot": {
      "root": "apps/bot",
      "sourceRoot": "apps/bot/src",
      "projectType": "application",
      "prefix": "bot",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/bot",
            "main": "apps/bot/src/main.ts",
            "tsConfig": "apps/bot/tsconfig.app.json",
            "assets": ["apps/bot/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/bot/src/environments/environment.ts",
                  "with": "apps/bot/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "bot:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/bot/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bot"],
          "options": {
            "jestConfig": "apps/bot/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "multi-chain": {
      "root": "libs/multi-chain",
      "sourceRoot": "libs/multi-chain/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/multi-chain/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/multi-chain"],
          "options": {
            "jestConfig": "libs/multi-chain/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redirect-generator": {
      "root": "libs/redirect-generator",
      "sourceRoot": "libs/redirect-generator/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/redirect-generator/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/redirect-generator"],
          "options": {
            "jestConfig": "libs/redirect-generator/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "interfaces": {
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/interfaces/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/interfaces"],
          "options": {
            "jestConfig": "libs/interfaces/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "trust-wallet-assets": {
      "root": "libs/trust-wallet-assets",
      "sourceRoot": "libs/trust-wallet-assets/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/trust-wallet-assets/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/trust-wallet-assets"],
          "options": {
            "jestConfig": "libs/trust-wallet-assets/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "prefix": "frontend",
      "targets": {
        "build-test-env": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cd apps/frontend && ../../node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production ../../node_modules/webpack/bin/webpack.js",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp apps/frontend/public -r dist/apps/frontend"
            ]
          },
          "outputs": ["{options.outputPath}"],
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.test.ts"
                }
              ]
            }
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cd apps/frontend && ../../node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production ../../node_modules/webpack/bin/webpack.js",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp apps/frontend/public -r dist/apps/frontend"
            ]
          },
          "outputs": ["{options.outputPath}"],
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "cd apps/frontend && ../../node_modules/webpack/bin/webpack.js serve"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/frontend/**/*.ts"]
          }
        },
        "check": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn svelte-check",
            "cwd": "apps/frontend"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/frontend"],
          "options": {
            "jestConfig": "apps/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
