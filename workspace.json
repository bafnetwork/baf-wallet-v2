    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "prefix": "frontend",
      "targets": {
        "build-deploy-test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npm run client-gen",
              "cd apps/frontend && mkdir -p public/build && $(npm bin)/webpack --node-env test",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp apps/frontend/public/ -r dist/apps/frontend"
            ],
            "parallel": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cd apps/frontend && $(npm bin)/cross-env NODE_ENV=production $(npm bin)/webpack",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp apps/frontend/public -r dist/apps/frontend"
            ]
          },
          "outputs": [
            "{options.outputPath}"
          ],
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "cd apps/frontend && $(npm bin)/cross-env NODE_ENV=development $(npm bin)/webpack serve"
          }
        },
        "serve+api": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cd apps/frontend && $(npm bin)/webpack serve",
              "nx serve api"
            ],
            "parallel": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend/**/*.ts"
            ]
          }
        },
        "check": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "$(npm bin)/svelte-check",
            "cwd": "apps/frontend"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/frontend"
          ],
          "options": {
            "jestConfig": "apps/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
