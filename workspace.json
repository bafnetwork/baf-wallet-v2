{
  "version": 2,
  "projects": {
    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nxext/svelte:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/frontend",
            "entryFile": "apps/frontend/src/main.ts",
            "tsConfig": "apps/frontend/tsconfig.app.json",
            "svelteConfig": "apps/frontend/svelte.config.js",
            "rollupConfig": "apps/frontend/rollup.config.js",
            "assets": [
              {
                "glob": "/*",
                "input": "apps/frontend/public/**",
                "output": "./"
              }
            ]
          },
          "configurations": {
            "production": {
              "dev": false
            }
          }
        },
        "serve": {
          "executor": "@nxext/svelte:build",
          "options": {
            "outputPath": "dist/apps/frontend",
            "entryFile": "apps/frontend/src/main.ts",
            "tsConfig": "apps/frontend/tsconfig.app.json",
            "svelteConfig": "apps/frontend/svelte.config.js",
            "rollupConfig": "apps/frontend/rollup.config.js",
            "assets": [
              {
                "glob": "/*",
                "input": "apps/frontend/public/**",
                "output": "./"
              }
            ],
            "watch": true,
            "serve": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "check": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "svelte-check",
            "cwd": "apps/frontend"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/frontend"
          ],
          "options": {
            "jestConfig": "apps/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "frontend-e2e": {
      "root": "apps/frontend-e2e",
      "sourceRoot": "apps/frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontend-e2e/cypress.json",
            "tsConfig": "apps/frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "tsoa": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "rm -r apps/api/build; mkdir apps/api/build"
              },
              {
                "command": "yarn tsoa --configuration apps/api/tsoa.json spec-and-routes && tsc"
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-client": {
      "root": "libs/api-client",
      "sourceRoot": "libs/api-client/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-client/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/api-client"
          ],
          "options": {
            "jestConfig": "libs/api-client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "autogen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "rm -r libs/api-client/src/build; mkdir libs/api-client/src/build"
              },
              {
                "command": "yarn openapi-generator-cli generate -i ./swagger/swagger.json --generator-name typescript-fetch -o libs/api-client/src/build"
              }
            ]
          }
        }
      }
    },
    "bot": {
      "root": "apps/bot",
      "sourceRoot": "apps/bot/src",
      "projectType": "application",
      "prefix": "bot",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/bot",
            "main": "apps/bot/src/main.ts",
            "tsConfig": "apps/bot/tsconfig.app.json",
            "assets": [
              "apps/bot/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/bot/src/environments/environment.ts",
                  "with": "apps/bot/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "bot:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bot/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/bot"
          ],
          "options": {
            "jestConfig": "apps/bot/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "api"
}
