          "options": {
            "jestConfig": "libs/redirect-generator/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "interfaces": {
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/interfaces/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/interfaces"],
          "options": {
            "jestConfig": "libs/interfaces/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "trust-wallet-assets": {
      "root": "libs/trust-wallet-assets",
      "sourceRoot": "libs/trust-wallet-assets/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/trust-wallet-assets/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/trust-wallet-assets"],
          "options": {
            "jestConfig": "libs/trust-wallet-assets/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "prefix": "frontend",
      "targets": {
        "build-test-env": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npm run client-gen",
              "cd apps/frontend && mkdir -p public/build && $(npm bin)/cross-env NODE_ENV=production $(npm bin)/webpack",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp apps/frontend/public/ -r dist/apps/frontend",
              "cp apps/frontend/public/build -r dist/apps/frontend/build"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.test.ts"
                }
              ]
            }
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cd apps/frontend && $(npm bin)/cross-env NODE_ENV=production $(npm bin)/webpack",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp apps/frontend/public -r dist/apps/frontend"
            ]
          },
          "outputs": ["{options.outputPath}"],
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "cd apps/frontend && $(npm bin)/webpack serve"
          }
        },
        "serve+api": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cd apps/frontend && $(npm bin)/webpack serve",
              "nx serve api"
            ],
            "parallel": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/frontend/**/*.ts"] }
        },
        "check": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "$(npm bin)/svelte-check",
            "cwd": "apps/frontend"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/frontend"],
          "options": {
            "jestConfig": "apps/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "near": {
      "root": "libs/near",
      "sourceRoot": "libs/near/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/near/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
